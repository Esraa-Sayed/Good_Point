<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F49C3B" />
            <size android:width="@dimen/_9sdp" />
            <corners
                android:bottomLeftRadius="700dp"
                android:bottomRightRadius="700dp"/>
        </shape>
    </item>
    <item android:top="230px">
        <shape>
            <solid android:color="#ffffff" />
        </shape>
    </item>
    <item android:top="500dp" android:left="300dp" android:right="0dp" android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#F49C3B" />
            <size
                android:width="1dp"
                android:height="1dp"/>
            <corners
                android:topLeftRadius="@dimen/_200sdp"/>
        </shape>
    </item>
</layer-list>
