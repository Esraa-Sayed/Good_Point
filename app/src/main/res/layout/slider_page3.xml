<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:startColor="#FFEB3B"
    android:endColor="#FF9800"
    android:background="@color/colorPrimary"
    android:backgroundTint="#ECCB26"
    android:foregroundTint="#7C1010">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView13"
        android:fontFamily="@font/roboto_bold"
        android:text="Find your missed"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/_49sdp"
        android:layout_marginLeft="@dimen/_70sdp"
        android:textAllCaps="true"
        android:textColor="#0F1010"
        android:textSize="@dimen/_20ssp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_bold"
        android:text="item easy"
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginLeft="@dimen/_110sdp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:layout_below="@+id/textView4"
        android:textColor="#0F1010"
        android:textSize="@dimen/_20ssp"
  />
    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="@dimen/_160sdp"
        android:layout_height="@dimen/_150sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginLeft="@dimen/_70sdp"
        app:srcCompat="@drawable/circle" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="@dimen/_110sdp"
        android:layout_height="@dimen/_120sdp"
        android:layout_marginTop="@dimen/_35sdp"
        android:layout_marginLeft="@dimen/_90sdp"
        app:srcCompat="@drawable/ic_baseline_phone_android_24" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginTop="@dimen/_49sdp"
        android:layout_marginLeft="@dimen/_110sdp"
        app:srcCompat="@drawable/ic_baseline_search_241" />


</RelativeLayout>